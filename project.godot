config_version=5

[application]

config/name="Societies"
config/tags=PackedStringArray("major_project")
config/features=PackedStringArray("4.1", "Mobile")
boot_splash/bg_color=Color(0.0784314, 0.0784314, 0.0784314, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

DialogueGlobals="*res://dialogue/dialogue_globals.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogic]

layout/mode=1
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
settings/text_speed=0.01
text/skippable=true
text/skippable_delay=0.1
text/autoadvance_delay=1.0
text/hide_empty_textbox=true
save/autosave_delay=60.0
text/autopauses={
"!": 0.1,
",": 0.1,
".": 0.1,
"?": 0.1
}
variables={}
text/input_action="dialogic_default_action"
glossary/default_case_sensitive=true
save/autosave=false

[dialogue_manager]

general/states=["DialogueGlobals"]
general/custom_test_scene_path="res://objects/dialogue_balloon/test_dialogue_runner.tscn"

[display]

window/size/mode=2
window/size/initial_position_type=3
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[filesystem]

import/blender/enabled=false

