; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Societies"
config/tags=PackedStringArray("major_project")
run/main_scene="res://global/blank.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Scorpion Games/Societies"
config/auto_accept_quit=false
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/bg_color=Color(0.14902, 0.14902, 0.14902, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

SaveManager="*res://global/save_manager.gd"
SceneManager="*res://global/scene_manager.tscn"
DialogueGlobals="*res://dialogue/dialogue_globals.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Utils="*res://global/utils.gd"
Pathfinder="*res://world/pathfinding/pathfinder.tscn"
Tasks="*res://global/tasks.gd"
Globals="*res://global/globals.gd"

[dialogue_manager]

general/states=["DialogueGlobals", "Tasks"]
general/custom_test_scene_path="res://objects/dialogue_balloon/test_dialogue_runner.tscn"

[display]

window/size/mode=2
window/size/initial_position_type=3
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/csv-data-importer/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg")

[filesystem]

import/blender/enabled=false

[importer_defaults]

com.timothyqiu.godot-csv-importer={
"delimiter": 0,
"detect_numbers": true,
"force_float": false,
"headers": false
}

